<Application x:Class="OneCardSln.OneCardClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="LoginWindow.xaml"
             DispatcherUnhandledException="Application_DispatcherUnhandledException">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/Style/ValidationTemplate.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Style/ButtonTemplate.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Style/WindowStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Style/MiscStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Resources/Style/TreeViewStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
